.header{
  width: 100%;
  height: 200rpx;
}
.header image{
  width: 100%;
  height: 100%;
}
.body,.note{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.img{
  width: 150rpx;
  height: 150rpx;
}
.img image{
  width: 100%;
  height: 100%;
}
.title{
  font-size: 40rpx;
  font-weight: bold;
  margin: 20px 0;
  margin-bottom: 20px;
  width: 80%;
  line-height: 60rpx;
}
.marks{
  font-size: 12px;
    color: #333;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    padding: 10px 20px;
    margin-bottom: 10px;
}
.url{
  color: #FF4B57;
  text-decoration: underline;
  line-height: 80rpx;
}
.url span{
  font-size: 50rpx;
}
.botm{
  position: fixed;
  bottom: 0;
  width: 100%;
}
.body{
  top: 20px;
}