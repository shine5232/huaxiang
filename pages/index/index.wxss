page {
  background: #eee;
}

.content-boy {
  width: 94%;
  margin: 0 auto;
}

swiper {
  height: 320rpx;
}

.swiper-item {
  width: 100%;
  height: 100%;
}

.banner-img {
  width: 100%;
  height: 100%;
}

.content-boy .body-item {
  margin: 30rpx 0;
}

.content-boy .label {
  background: #fff;
  border-radius: 5px;
  padding: 30rpx 20rpx;
}

.label .label-title {
  display: flex;
  align-items: center;
}

.label-title .label-title-icon {
  margin-right: 20rpx;
  height: 36rpx;
  width: 8rpx;
  border-radius: 2px;
  background-color: #eee;
}

.red {
  background-color: #ff6947;
}

.blue {
  background-color: #5886FF;
}

.active {
  color: #ff6947;
}

.active-back {
  background: #ffebe6;
}

.regist {
  color: #5886FF;
}

.regist-back {
  background: #e7efff;
}

.label-title .label-title-text {
  color: #333;
}

.label-item {
  margin: 20px 0 5px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.label-item-i {
  padding: 20rpx;
  border-radius: 5px;
  width: 42%;
}

.label-item-text {
  padding-bottom: 10px;
  /*font-weight: bold;*/
}

.label-item-img {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.label-item-img image {
  width: 70px;
  height: 50px;
}

.kefu {
  display: none;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  position: absolute;
  top: 56%;
  right: 10px;
  box-shadow: 0 1px 4px #737f9e;
}

.kefu-btn {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background: #5886FF;
  font-size: 14px;
  line-height: 20px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  opacity: .8;
}

.label-items {
  background: #fff;
  margin: 5px 0;
  border-radius: 10rpx;
}

.label-items .label-item-i {
  padding: 20rpx;
  border-radius: 5px;
  width: initial;
}

.label-item-i .i-title {
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.i-title image {
  width: 40rpx;
  height: 40rpx;
}

.i-title .text-name {
  margin-left: 14rpx;
  font-weight: bold;
  color: #333;
}

.i-content {
  padding: 10rpx 0;
}

.i-content .i-content-item {
  display: flex;
  align-items: center;
  color: #999;
  font-size: 24rpx;
  margin: 10rpx 0;
}

.i-content-item .item-name {
  margin-left: 10rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  flex: 1;
}

.i-content-item .item-dis {
  text-align: right;
  width: 100rpx;
}