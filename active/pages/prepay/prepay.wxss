.bodys{
  margin-top: 15%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.note-body {
  width: 70%;
  font-size: 28rpx;
  line-height: 64rpx;
  color: #333;
}

.note-item .money {
  font-size: 36rpx;
